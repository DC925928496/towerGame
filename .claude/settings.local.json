{
  "env": {
    "CLAUDE_BASH_NO_LOGIN": "1"
  },
  "permissions": {
    "allow": [
      "Bash(dir:*)",
      "Bash(findstr:*)",
      "Bash(pip install:*)",
      "Bash(python -m py_compile:*)",
      "Bash(timeout 3 python:*)",
      "Bash(pip show:*)",
      "Bash(python:*)",
      "SlashCommand(/tr:withScout)",
      "Bash(rm:*)",
      "Bash(del:*)",
      "Bash(cat -n)",
      "Bash(cat:*)",
      "Bash(find:*)"
    ],
    "deny": [],
    "ask": []
  }
}
